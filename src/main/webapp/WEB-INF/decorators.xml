<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/main.jsp</pattern>
		<pattern>/main2.jsp</pattern>
		<pattern>/main2_user.jsp</pattern>
		<pattern>/main2_highchart.jsp</pattern>
	</excludes>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
	
	<decorator name="t1" page="t1.jsp">
		<pattern>/upload.jsp</pattern>
	</decorator>
</decorators>

<?xml version="1.0" encoding="UTF-8"?> 
<workbook> 
   <worksheet name="Sheet1"> 
    <section startRow="0" endRow="0"/> 
    <loop startRow="1" endRow="1" items="cards" var="card" varType="com.bnu.card.web.form.CardInfoXlsVo"> 
    <section startRow="1" endRow="1"> 
    <mapping row="1" col="0">card.name</mapping>
    <mapping row="1" col="1">card.identityCard</mapping>
    <mapping row="1" col="2">card.gender</mapping>
    <mapping row="1" col="3">card.address</mapping>
    <mapping row="1" col="5">card.job</mapping>
    <mapping row="1" col="6">card.academe</mapping>
    </section> 
    <loopbreakcondition> 
     <rowcheck offset="0"> 
        <cellcheck offset="0"></cellcheck> 
      </rowcheck> 
    </loopbreakcondition> 
    </loop> 
    </worksheet> 
</workbook> 